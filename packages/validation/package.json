{
  "name": "@mintlify/validation",
  "version": "0.1.4",
  "description": "Validates mint.json files",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "mintlify",
    "mint"
  ],
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "prepare": "npm run build"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "@/utils/(.*)": "<rootDir>/src/utils/$1",
      "@/schemas/(.*)": "<rootDir>/src/schemas/$1",
      "@/types/(.*)": "<rootDir>/src/types/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/mint/tree/main/packages/validation"
  },
  "bugs": {
    "url": "https://github.com/mintlify/mint/issues"
  },
  "author": "Mintlify, Inc.",
  "license": "Elastic-2.0",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.8.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "zod": "^3.22.3",
    "zod-to-json-schema": "^3.20.2"
  }
}
